{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noImplicitAny": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/ViewPost.js",
    "components/TrendingList.js",
    "components/PostList.js",
    "components/PostBox.js",
    "components/Post.js",
    "components/PopularUser.js",
    "node_modules/@sendbird/uikit-react/index.d.ts"
  ],
  "exclude": ["node_modules"]
}
